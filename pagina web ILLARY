/* Estilos para los recuadros de las secciones */
.card {
  background-color: #f9f9f9;
  border-radius: 10px;
  padding: 20px;
  margin-bottom: 30px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
}

.card h2 {
  font-size: 24px;
  color: #333;
}

.card .content {
  margin-top: 20px; /* AÃ±adir un poco de espacio por encima de las noticias */
}

.card .content .image {
  margin-bottom: 10px;
}

.card img {
  max-width: calc(100% - 40px); /* TamaÃ±o mÃ¡ximo de la imagen */
  max-height: 300px; /* Altura mÃ¡xima de la imagen */
  border-radius: 5px;
}

.card .content .description p {
  font-size: 18px;
  color: #666;
  text-align: center;
  text-decoration: none; /* Eliminar subrayado */
  cursor: default; /* Cambiar cursor a predeterminado para indicar que no es un enlace */
}

/* Ajustes para las noticias */
.news {
  display: flex;
  justify-content: space-between;
}

.news-item {
  width: calc(25% - 20px); /* 25% del ancho del contenedor con margen */
